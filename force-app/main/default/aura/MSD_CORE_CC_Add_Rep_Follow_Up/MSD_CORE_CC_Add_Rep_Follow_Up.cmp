<aura:component extends="c:ComponentUtils" controller="MSD_CORE_CC_QuickActionButtonsController" implements="lightning:actionOverride,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickActionWithoutHeader" access="global">
    
    <aura:attribute name="recordId" type="String"/>
    <aura:attribute name="record" type="Object" />
    <aura:attribute name="simpleRecord" type="Object" />
    <aura:attribute name="userProfile" type="String"/>
    <lightning:workspaceAPI aura:id="workspace"/>
    <aura:attribute name="objCase" type="Object" />
    <aura:attribute name="recordLoadError" type="String"/>
	<force:recordData aura:id="recordLoader"
            recordId="{!v.recordId}"
            fields="RecordTypeId, RecordType.Name"
            targetFields="{!v.objCase}"
            targetError="{!v.recordLoadError}"
            mode="VIEW"
            />
    <aura:handler name="MSD_CORE_CC_CloseModalEvt" event="c:MSD_CORE_CC_CloseModalEvt" action="{!c.handleCloseEvent}"/>
    <button style="top: 9px; z-index: 1; float: right;width: 85px;margin-right: 15px;padding: 0;" class="slds-button slds-button_brand" onclick="{!c.handleQuickActionMenuSelect}"> 
           <span class="slds-truncate slds-m-left--xxx-small" >RepFollowUp</span> 
     </button> 
    
</aura:component>