<apex:page standardcontroller="Call2_vod__c" extensions="VeevaCommonMessageExtension" title="{!TranslatedEdit} {!Call2_vod__c.Name}">

        <apex:pageBlock rendered="{!IF(Call2_vod__c.Status_vod__c == 'Submitted_vod', true, false)}" mode="maindetail">
